platform :ios, '11.0'

target 'Klotski' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Klotski
  pod 'CocoaLumberjack/Swift'
  pod 'Pageboy'

  target 'KlotskiTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'KlotskiUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
